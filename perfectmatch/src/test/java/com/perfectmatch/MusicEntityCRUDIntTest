
import com.natixis.etrading.fxhub.business.Account;

public class AccountEntityCRUDIntTest extends EntityCRUDIntTest<Account, String>{
		
	public AccountEntityCRUDIntTest() {
		super(Account.class);
	}
	

	@Override
	public String getID() {
		return entity.getId();
	}

	@Override
	public void buildEntity() {
		entity = new Account();
		entity.setName("SellerName");
		entity.setEnabled(Boolean.TRUE);
		entity.setBypassCreditCheck(Boolean.TRUE);
		entity.setInternalEntity(Boolean.TRUE);
		entity.setSettlementVoiceRate(2.0);

	}

	@Override
	public void updateEntity() {
		entity.setShortname("client Name");
		entity.setIm1("client Im1");
		entity.setMnemo("client Mnemo");
	}

}
